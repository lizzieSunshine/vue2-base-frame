{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/essential",
    "@vue/prettier"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "quotes": [1, "single", "avoid-escape"],
    "semi": [2, "always"],
    "comma-dangle": 1,
    "no-var": 1,
    "prettier/prettier": "off"
  }
}